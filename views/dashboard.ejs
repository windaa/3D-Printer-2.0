        <!DOCTYPE html>
        <html lang="en">

        <head>

          <meta charset="utf-8">
          <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
          <meta name="description" content="">
          <meta name="author" content="">

          <title>3DP-INF</title>

          <!-- Bootstrap core CSS -->
          <!-- Latest compiled and minified CSS -->
          <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

          <!-- Optional theme -->
          <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css" integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp" crossorigin="anonymous">

          <!-- Latest compiled and minified JavaScript -->
          <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
          <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
          <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.bundle.min.js"></script>
          <link rel="icon" type="image/png" href="public/images/fav.png">

          <!-- Custom styles for this template -->
          <style>
            body {
              padding-top: 54px;
            }
            @media (min-width: 992px) {
              body {
                padding-top: 56px;
              }
            }

          </style>
        </head>

        <body>
          <!-- Page Content -->
          <div class="container">
            <div class="row">
              <div class="col-lg-12 text-left">
               <h1 class="mt-5">Dashboard</h1>
             </div>
           </div>
           <div class="row">
            <div class="col-lg-12 text-left">
             <h5 class="mt-1">Material</h5>
             <table class="table">
              <tr>
                <th><p>Material ID</p></th>
                <th><p>Material Name</p></th>
                <th><p>Filament Weight (g)</p></th>
              </tr>
              <tr>
                <% for(var i=0; i < datas.length; i++) { %>
                <td><%= datas[i].materialID %></td>
                <td><%= datas[i].materialName %></td>
                <td><%= datas[i].filamentWeight %></td>
              </tr>
              <% } %>
            </table>
            <form action="/" method="GET">
              <input type="submit" id="updateButton" value="Update Table" class="btn btn-primary">
            </form>
          </div>
        </div>

        <div class="row">
          <div class="col-lg-12 text-left">
           <h5 class="mt-5">Upload Gcode</h5>
           <form action="/submit" method="POST">
            <div class="form-group">
              <label for="email">Material ID:</label>
              <input type="MatID" class="form-control" id="MatID" placeholder="Enter material ID" name="MatID">
            </div>
            <div class="form-group">
              <label for="exampleFormControlFile1">Choose Gcode file:</label>
              <input type="file" class="form-control-file" id="exampleFormControlFile1">
            </div>
            <input type="submit" id="submitButton" value="Submit" class="btn btn-primary">
          </form>
        </div>
      </div>

      <div class="row">
        <div class="col-lg-12 text-left">
          <h5 class="mt-3"><b>Upload Status:</b></h5>
          <table class="table">
            <tr>
              <th><p>File Name</p></th>
              <th><p>Length</p></th>
              <th><p>Weight</p></th>
              <th><p>Status</p></th>
            </tr>
            <tr>
              <td><%= filename %></td>
              <td><%= lengthEjs %></td>
              <td><%= weightEjs %></td>
              <td><%= available %></td>
            </tr>
          </table>
        </div>
      </div>

    </div>
  </body>

  </html>
